{
	"manifest_version": 2,
	"name": "Cache Cleaner",
	"version": "0.1",

	"browser_action": {
		"default_title": "test",
		"default_popup": "popup.html",
		"default_icon": "img/icon.png"
	},

	"background": {
		"scripts": ["background.js"]
	},
	// Permissions
	"permissions": [
		"tabs",
		"browsingData"
	]
}